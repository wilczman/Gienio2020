import openpyxl

wb = openpyxl.load_workbook('Przepis.xlsx')
wb.get_sheet_names()
sheet = wb.get_sheet_by_name('Sheet1')
sheet['A1'].value
wb.save('PrzepisSaved.xlsx')
sheet.title
sheet.title = 'My sheet name'
sheet.cell(row = 1, column = 3).value

for i in range(1,5): print(sheet.cell(row = i, column = 3).value)

print(sheet.max_row)
print(sheet.max_column)


